{
  "name": "StarterNode",
  "version": "1.0.0",
  "description": "Quick node starter app with basic needs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --watch --testPathIgnorePatterns ./_end2end",
    "test-cov": "jest --coverage --testPathIgnorePatterns ./_end2end",
    "acceptance": "jest ./_end2end",
    "lint": "node_modules/eslint/bin/eslint.js .",
    "pre-commit": "npm run lint && npm run test-cov && npm run acceptance"
  },
  "author": "Luke Perry",
  "license": "ISC",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "mongodb": "^3.5.5",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "jest": "^25.1.0",
    "jest-extended": "0.11.5",
    "mocked-env": "^1.3.2",
    "nock": "^12.0.0",
    "supertest": "^4.0.2"
  }
}
